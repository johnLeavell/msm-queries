<h1>List of all directors</h1>

<table border="1">
  <% @list_of_directors.each do |dir| %>
    <tr>
      <td><%= dir.id %></td>
      <td>
        <img src="http://robohash.org/<%= dir.image %>">
      </td>
      <td><%= dir.name %></td>
      <td><%= dir.dob %></td>
      <td><a href="/directors/<%= dir.id %>">
          Show details</a>
      </td>
    </tr>
  <% end %>
</table>
